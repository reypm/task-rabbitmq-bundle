<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <mapped-superclass name="Yceruto\TaskRabbitMqBundle\Model\Task">
        <field name="name" column="name" type="string" />
        <field name="status" column="status" type="integer" />
        <field name="jobsCount" column="jobs_count" type="integer" />
        <field name="jobsProcessedCount" column="jobs_processed_count" type="integer" />
    </mapped-superclass>

</doctrine-mapping>
